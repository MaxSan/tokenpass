{
    "name": "tokenly/accounts",
    "description": "Tokenly Accounts and TCA provider.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "lucadegasperi/oauth2-server-laravel": "dev-master@dev",
        "tokenly/laravel-api-provider": "dev-master",
        "cryptonaut/illuminate-html": "dev-master",
        "guzzlehttp/guzzle": "~6",
        "tokenly/consul-health-daemon": "dev-master",
		"linusu/bitcoin-address-validator": "~0",
		"tokenly/xchain-client": "@dev",
		"tokenly/token-controlled-access": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cryptonaut420/html"
        }
    ],    
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "TKAccounts\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/testlib/",
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true
}
